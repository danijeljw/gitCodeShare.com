    "@types/express-session": "^1.15.11",
    "@types/highlight.js": "^9.12.3",
    "@types/http-proxy-middleware": "^0.19.0",
    "@types/lodash": "^4.14.118",
    "@types/morgan": "^1.7.35",
    "@types/next": "^7.0.5",
